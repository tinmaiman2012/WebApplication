.msm-tr-record:hover
  background #d9edf7 !important

.panel-heading
  color #8a6d3b
  background-color #fcf8e3
  border-color #ebccd1

.panel-heading-add-list
  padding 10px 15px
  border-bottom 1px solid transparent
  border-top-right-radius 3px
  border-top-left-radius 3px
  color #ffffff
  background-color #428bca
  border-color #bce8f1

.msm-parent-table-song
  max-height 500px
  overflow auto !important

.panel-heading-list
  padding: 10px 15px
  border-bottom: 1px solid transparent
  border-top-right-radius 3px
  border-top-left-radius 3px
  color #8a6d3b
  background-color #fcf8e3
  border-color #ebccd1

.msm-tr-title
  color black
  font-family bold
  font-size 1.5em

.msm-td-action
  text-align center
  width 120px

.odd
  background #f5f5f5 !important

.even
.msm-odd-list
  background: #f5f5f5

.msm-div-parent-of-heading
  width 100%
  max-width 700px
  margin-left 5px
  /*overflow hidden*/
  height 55px

.msm-box-control-small
  text-align left
  position relative
  height 40px
  border-radius: 5px
  margin-right 10px
  margin-top - 10px
  float right;
  width 0.01%
  overflow hidden
  background aliceblue
  webkit-transition width 0.3s
  transition width 0.3s

li
  list-style none

.msm-parent-box-small
  position relative
  float: right;
  margin-right 5px !important
  width 100%
  display none
  font-size 2em
  text-align center
  margin-top - 30px

.msm-parent-box-small:hover
  cursor pointer

.msm-box-control
  text-align center
  float right
  width 100%
  margin-top 5px
  max-width 180px
  position relative

