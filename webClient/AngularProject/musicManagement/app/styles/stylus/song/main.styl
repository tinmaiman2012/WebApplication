.msm-box-control-button
  margin-left 5px

.clsChose
  color green !important
  background #fcf8e3 !important

@media screen and (min-width: 480px)
  .msm-min-screen-show
    display none !important

@media screen and (max-width: 768px)
  .msm-box-control
    display none

  .msm-parent-box-small
    display block

  .msm-div-parent-of-heading
    width 100%
    max-width 700px
    margin-left 5px
    overflow hidden
    height 55px

@media screen and (max-width: 480px)
  .msm-td-action_text
    display none

  .msm-min-screen-show
    display block

  .msm-min-screen-hide
    display none

  .msm-div-parent-of-heading
    height 45px !important

